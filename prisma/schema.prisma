generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model driver {
  id              Int    @id @default(autoincrement())
  driverFirstName String
  driverLastName  String
  plate           String
  carType         String
  sex             String
  phoneNumber     String
}
